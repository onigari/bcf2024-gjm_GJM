config_version=5

[application]

config/name="Legacy of Valor"
run/main_scene="res://scenes/Main_menu.tscn"
config/features=PackedStringArray("4.3", "Mobile")
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

Game="*res://autoload/Game.gd"
GameManager="*res://autoload/game_manager.gd"
KeyManager="res://autoload/key_manager.gd"
LevelManager="*res://autoload/level_manager.gd"
OptionsManager="*res://autoload/options_manager.gd"
SoundManager="*res://autoload/sound_manager.tscn"
Utils="*res://autoload/Utils.gd"
HealthManager="*res://autoload/health_manager.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[dialogue_manager]

general/states=["Game", "GameManager", "KeyManager", "SoundManager", "OptionsManager", "LevelManager", "Utils", "HealthManager"]
general/wrap_lines=true

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"
mouse_cursor/tooltip_position_offset=Vector2(12, 12)

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

